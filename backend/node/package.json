	"scripts": {
		"db:generate": "NODE_OPTIONS='-r dotenv/config' drizzle-kit generate",
		"db:migrate": "NODE_OPTIONS='-r dotenv/config' drizzle-kit migrate",
		"db:seed": "node -r dotenv/config -r @swc-node/register src/infrastructure/dal/seeds.ts",
		"dev:express": "node -r dotenv/config -r @swc-node/register --watch src/infrastructure/server/express/index.ts",
		"dev:fastify": "node -r dotenv/config -r @swc-node/register --watch src/infrastructure/server/fastify/index.ts",
		"dev": "run-s dev:express",
		"clean": "rimraf dist",
		"build": "run-s clean build:ts",
		"build:ts": "tsc -p tsconfig.build.json",
		"start:express": "node -r ./src/infrastructure/server/express/telemetry.js dist/infrastructure/server/express/index.js",
		"start": "run-s start:express",
		"lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
		"lint:fix": "eslint --ignore-path .gitignore --ext .js,.ts --fix .",
		"typecheck": "tsc --noEmit",
		"prettier": "prettier --ignore-path .gitignore --ignore-path .prettierignore --check .",
		"prettier:write": "prettier --ignore-path .gitignore --ignore-path .prettierignore --write .",
		"format": "run-s lint:fix prettier:write typecheck",
		"test": "NODE_ENV=testing jest --detectOpenHandles",
		"test:watch": "NODE_ENV=testing jest --watch --detectOpenHandles"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.990.0",
		"@aws-sdk/s3-request-presigner": "3.991.0",
		"@lukeed/ms": "2.0.2",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/exporter-logs-otlp-grpc": "^0.208.0",
		"@opentelemetry/exporter-metrics-otlp-grpc": "^0.205.0",
		"@opentelemetry/exporter-trace-otlp-grpc": "^0.206.0",
		"@opentelemetry/instrumentation-express": "^0.55.0",
		"@opentelemetry/instrumentation-http": "^0.208.0",
		"@opentelemetry/instrumentation-knex": "^0.51.0",
		"@opentelemetry/instrumentation-pg": "^0.58.0",
		"@opentelemetry/instrumentation-pino": "^0.52.0",
		"@opentelemetry/resources": "^2.5.1",
		"@opentelemetry/sdk-metrics": "^2.5.1",
		"@opentelemetry/sdk-node": "^0.211.0",
		"@opentelemetry/sdk-trace-node": "^2.4.0",
		"@opentelemetry/semantic-conventions": "^1.39.0",
		"cache-manager": "7.2.4",
		"cache-manager-ioredis": "2.1.0",
		"drizzle-orm": "^0.44.7",
		"express": "^5.2.1",
		"fastify": "^5.7.4",
		"http-status-codes": "^2.3.0",
		"ioredis": "5.9.2",
		"multer": "2.0.2",
		"pg": "8.16.3",
		"pino": "^10.3.0",
		"pino-pretty": "^13.1.1",
		"swagger-ui-express": "^5.0.1",
		"ts-japi": "^1.12.1",
		"yaml": "^2.8.2",
		"zod": "^4.1.13"
	},
