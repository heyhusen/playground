	"scripts": {
		"migrate:list": "NODE_OPTIONS='-r dotenv/config' knex migrate:list",
		"migrate:make": "NODE_OPTIONS='-r dotenv/config' knex migrate:make",
		"migrate:latest": "NODE_OPTIONS='-r dotenv/config' knex migrate:latest",
		"migrate:rollback": "NODE_OPTIONS='-r dotenv/config' knex migrate:rollback",
		"migrate:up": "NODE_OPTIONS='-r dotenv/config' knex migrate:up",
		"migrate:down": "NODE_OPTIONS='-r dotenv/config' knex migrate:down",
		"seed:make": "NODE_OPTIONS='-r dotenv/config' knex seed:make",
		"seed:run": "NODE_OPTIONS='-r dotenv/config' knex seed:run",
		"dev": "run-s dev:express",
		"dev:express": "tsnd -r dotenv/config src/infrastructure/express/index.ts",
		"clean": "rimraf dist",
		"build": "run-s clean build:ts",
		"build:ts": "tsc",
		"start": "run-s start:express",
		"start:express": "node dist/infrastructure/express/index.js",
		"lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
		"lint:fix": "eslint --ignore-path .gitignore --ext .js,.ts --fix .",
		"prettier": "prettier --check .",
		"prettier:write": "prettier --write .",
		"format": "run-s lint:fix prettier:write",
		"test": "NODE_ENV=testing jest",
		"test:watch": "NODE_ENV=testing jest --watch"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.110.0",
		"@aws-sdk/s3-request-presigner": "3.110.0",
