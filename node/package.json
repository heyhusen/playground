		"@lukeed/ms": "2.0.0",
		"argon2": "0.28.5",
		"cache-manager": "4.0.1",
		"cache-manager-ioredis": "2.1.0",
		"fast-jwt": "1.5.4",
		"ioredis": "5.0.6",
		"knex": "2.1.0",
		"pg": "8.7.3",
		"winston": "3.8.1"
	},
	"devDependencies": {
		"@jest/types": "^28.1.1",
		"@tsconfig/node16-strictest": "1.0.2",
		"@types/cache-manager": "4.0.0",
		"@types/cache-manager-ioredis": "2.0.2",
		"@types/jest": "^28.1.1",
		"@types/node": "17.0.43",
		"@types/pg": "8.6.5",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "5.28.0",
		"@typescript-eslint/parser": "5.28.0",
		"dotenv": "16.0.1",
		"eslint": "8.17.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-prettier": "4.0.0",
		"jest": "28.1.1",
		"npm-run-all": "4.1.5",
		"prettier": "2.7.0",
		"rimraf": "3.0.2",
		"supertest": "^6.2.3",
		"ts-jest": "28.0.5",
		"ts-node": "10.8.1",
